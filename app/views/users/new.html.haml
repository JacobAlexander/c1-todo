%h1.h1
  Sign Up
.form-signup
  = render 'partials/errors', :obj => @user
  =form_for :user, :url=> sign_up_path, :html => {:class => 'form-g'} do |f|
    .form-i
      = f.label :name
      = f.text_field :name, :value => 'Jacob'
    .form-i
      = f.label :email
      = f.text_field :email
    .form-i
      = f.label :password
      = f.text_field :password
    .form-i
      = f.label :password_confirmation
      = f.text_field :password_confirmation
    .form-i
      = f.submit 'Create account'